      }
    },
    "ca89b6f0-8674-4985-8ba0-16a3ed6a09f8": {
      "type": "image-with-text",
      "blocks": {
        "5d6c4508-e7ef-4782-86d5-145d853477f3": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<link rel=\"stylesheet\" href=\"https:\/\/fonts.googleapis.com\/css2?family=Josefin+Sans:wght@300;400;700&display=swap\">\n<h4 class=\"image-with-text__heading h2 hello\" style=\"font-family: 'Josefin Sans';color:#000;font-weight:600;\">letter from founder<\/h4>"
