    "image-with-text-2": {
      "type": "image-with-text",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "heading": "WHAT WE BELIEVE",
            "heading_size": "h1",
            "heading_tag": "h2"
          }
        },
        "text": {
          "type": "text",
          "settings": {
            "text": "<p><strong>We believe<\/strong> in making effective products from fresh, organic* fruit and vegetables, the finest essential oils and safe synthetics.<\/p><p><strong>We invent<\/strong> our own products and fragrances. We make them fresh by hand using little or no preservative or packaging, using only vegetarian ingredients, and tell you when they were made.<\/p>",
            "text_size": "typeset2",
            "secondary_color": true
          }
        }
      },
