    "image-with-text-0": {
      "type": "image-with-text",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "heading": "We believe ethical practices should be business as usual",
            "heading_size": "h2",
            "heading_tag": "h2"
          }
        },
        "text": {
          "type": "text",
          "settings": {
            "text": "<p>At FunFormulae, we've never been keen on labeling ourselves as an \"ethical company.\" The term poses a challenge for us, as it seems to suggest that being mindful of people and the planet in our business practices is an exception, rather than the standard way of conducting business.<\/p>",
            "text_size": "typeset2",
            "secondary_color": true
          }
        }
      },
      "block_order": [
        "heading",
        "text"
      ],
      "settings": {
        "image": "shopify:\/\/shop_images\/KEW6701_copy_6b641da4-dc41-46b3-b580-ad79896fa550.jpg",
