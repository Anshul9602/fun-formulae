      "settings": {
        "custom_liquid": "<style>\n  .tabs .tab-titles li {\n    display: inline-block;\n    margin: 0 10px;\n    cursor: pointer;\n  }\n\n  .tabs .tab-content .tab-panel {\n    display: none;\n  }\n\n  .tabs .tab-content .tab-panel.active {\n    display: block;\n  }\n\n  /* General card styles */\n  .product-card {\n    border: 1px solid #ddd;\n    border-radius: 10px;\n    overflow: hidden;\n    text-align: center;\n    width: 280px;\n    margin: 20px;\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n    transition: transform 0.3s ease;\n  }\n\n  .product-card:hover {\n    transform: translateY(-10px);\n  }\n\n  /* Image styles */\n  .product-card__image img {\n    width: 100%;\n    height: auto;\n    border-bottom: 1px solid #ddd;\n  }\n\n  /* Product Info Section */\n  .product-card__info {\n    padding: 15px;\n  }\n\n  /* Title styling */\n  .product-card__title {\n    display: block;\n    font-size: 16px;\n    font-weight: bold;\n    color: #333;\n    text-decoration: none;\n    margin: 10px 0;\n    transition: color 0.2s ease;\n  }\n\n  .product-card__title:hover {\n    color: #007bff;\n  }\n\n  /* Price styling */\n  .product-card__price {\n    margin: 10px 0;\n  }\n\n  .product-card__price .price-regular {\n    font-size: 18px;\n    color: #333;\n  }\n\n  /* Button styles */\n  .product-card__button {\n    margin-top: 15px;\n  }\n\n  .add-to-cart {\n    background-color: #007bff;\n    color: #fff;\n    padding: 10px;\n    font-size: 14px;\n    border: none;\n    border-radius: 5px;\n    cursor: pointer;\n    width: 100%;\n  }\n\n  .add-to-cart:hover {\n    background-color: #0056b3;\n  }\n\n  /* Quick View Button */\n  .product-card__quick-view {\n    margin-top: 10px;\n  }\n\n  .quick-view-button {\n    background-color: #f8f9fa;\n    border: 1px solid #ddd;\n    padding: 8px 12px;\n    font-size: 14px;\n    border-radius: 5px;\n    cursor: pointer;\n    width: 100%;\n    transition: background-color 0.3s ease;\n  }\n\n  .quick-view-button:hover {\n    background-color: #e2e6ea;\n  }\n</style>\n\n<div class=\"tabs\">\n  <ul class=\"tab-titles\">\n    <li><a href=\"#face-care-tab\">Face Care</a></li>\n    <li><a href=\"#body-care-tab\">Body Care</a></li>\n    <li><a href=\"#hair-care-tab\">Hair Care</a></li>\n  </ul>\n  <div class=\"tab-content\">\n    <div id=\"face-care-tab\" class=\"tab-panel\">\n      {% case section.settings.selected_tab %}\n      {% when 'face-care' %}\n        {% assign collection = collections['face-care'] %}\n      {% when 'body-care' %}\n        {% assign collection = collections['body-care'] %}\n      {% when 'hair-care' %}\n        {% assign collection = collections['hair-care'] %}\n      {% else %}\n        {% assign collection = collections['face-care'] %}\n    {% endcase %}\n\n      <slider-component class=\"slider-mobile-gutter\">\n        <ul id=\"Slider-{{ section.id }}\"\n            class=\"product-grid grid{% if section.settings.columns_mobile == '1' %} grid--1-col-mobile{% endif %} grid--2-col{% if section.settings.collection == blank or products_to_display == 4 and section.settings.swipe_on_mobile == false %} grid--2-col-tablet grid--4-col-desktop{% else %} grid--3-col-tablet{% if products_to_display == 6 %} grid--3-col-desktop{% else %} grid--4-col-desktop{% endif %}{% endif %}{% if section.settings.collection.all_products_count > 2 and section.settings.swipe_on_mobile and section.settings.products_to_show > 2 %} slider slider--tablet{% endif %}{% if section.settings.collection == blank and section.settings.swipe_on_mobile %} slider slider--tablet{% endif %}\"\n            role=\"list\"\n          >\n            {%- for product in collection.products limit: section.settings.products_to_show -%}\n              <li id=\"Slide-{{ section.id }}-{{ forloop.index }}\"\n                  class=\"grid__item{% if section.settings.collection.all_products_count > 2 and section.settings.swipe_on_mobile and section.settings.products_to_show > 2 %} slider__slide{% endif %}\"\n                >\n                {% render 'card-product',\n                  card_product: product,\n                  card_collection: collection,\n                  media_size: section.settings.image_ratio,\n                  show_secondary_image: section.settings.show_secondary_image,\n                  show_quick_buy: section.settings.show_quick_buy,\n                  show_vendor: section.settings.show_vendor,\n                  show_rating: section.settings.show_rating,\n                  enable_quick_view: section.settings.enable_quick_view,\n                  enable_color_swatches: section.settings.enable_color_swatches,\n                  enable_countdown: section.settings.enable_countdown,\n                  enable_image_fill: section.settings.enable_image_fill\n                %}\n              </li>\n            {%- else -%}\n              {%- for i in (1..4) -%}\n                <li class=\"grid__item{% if section.settings.swipe_on_mobile %} slider__slide{% endif %}\">\n                  {% render 'card-product-placeholder', media_size: section.settings.image_ratio %}\n                </li>\n              {%- endfor -%}\n            {%- endfor -%}\n          </ul>\n      </slider-component>\n\n      {% if section.settings.show_view_all and collection.products_count > section.settings.products_to_show -%}\n        <div class=\"view-all center\">\n          <a href=\"{{ collection.url }}\"\n            class=\"button\"\n            aria-label=\"{{ 'sections.featured_collection.view_all_label' | t: collection_name: collection.title }}\"\n          >\n            {{ 'sections.featured_collection.view_all' | t }}\n          </a>\n        </div>\n      {% endif %}\n\n\n    </div>\n    <div id=\"body-care-tab\" class=\"tab-panel\">\n      {% for product in collections['body-care'].products %}\n      <!-- Display product here -->\n      <p>{{ product.title }}</p>\n      {% endfor %}\n    </div>\n    <div id=\"hair-care-tab\" class=\"tab-panel\">\n      {% for product in collections['hair-care'].products %}\n      <!-- Display product here -->\n      <p>{{ product.title }}</p>\n      {% endfor %}\n    </div>\n  </div>\n</div>\n<script>\n  document.querySelectorAll('.tab-titles li').forEach(function (tab) {\n    tab.addEventListener('click', function () {\n      document.querySelectorAll('.tab-panel').forEach(function (panel) {\n        panel.classList.remove('active');\n      });\n      document.querySelector(tab.querySelector('a').getAttribute('href')).classList.add('active');\n    });\n  });\n</script>",
